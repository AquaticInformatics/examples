<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  
  <appSettings>
    <add key="DataTimeRangeInMonthsFromNow" value="8"/>
    <add key="SaveBadHttpContentInAboluteDirectory" value="E:\Auckland\InvalidHttpFiles"/>
    <add key="LogTextFromDas" value="True"/>
  </appSettings>

  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date{MM-dd HH:mm:ss.fff} %-5level - %message%newline"/>
      </layout>
    </appender>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <file value="${ALLUSERSPROFILE}/Aquatic Informatics/AQUARIUS/AQDAS_Preprocessor.log"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="30"/>
      <maximumFileSize value="20MB"/>
      <countDirection value="1"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%property{pid}] %-5level - %message%newline"/>
      </layout>
    </appender>
    <root>
      <level value="DEBUG"/>
      <appender-ref ref="RollingFileAppender"/>
    </root>
  </log4net>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="log4net"
                          publicKeyToken="669e0ddf0bb1aa2a"
                          culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.0.8.0"
                         newVersion="1.2.15.0"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>